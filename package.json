{
  "name": "new-portfolio",
  "version": "1.0.0",
  "scripts": {
    "build:css:prod": "npx tailwindcss -i ./src/assets/css/style.css -o ./dist/assets/css/style.css --minify",
    "build:11ty": "npx @11ty/eleventy",
    "build": "npm run build:css:prod && npm run build:11ty",
    "build-ghpages": "npm run build:css:prod && npx @11ty/eleventy --pathprefix=/"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
